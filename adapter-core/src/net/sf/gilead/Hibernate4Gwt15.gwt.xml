<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	
	<!-- Additional source path -->
	<source path='pojo/base'/>
	<source path='pojo/java5'/>
	<source path='proxy'/>
	
	<!-- Proxy generator -->
	<generate-with class="net.sf.gilead.rebind.Gwt15ProxyGenerator">
		<when-type-assignable class="java.io.Serializable" />
	</generate-with>
</module>